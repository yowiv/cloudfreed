{
  "manifest_version": 3,
  "permissions": [
      "debugger"
  ],
  "name": "Turnstile Patcher",
  "version": "1",
  "background": {
      "service_worker": "background.js",
      "type": "module"
  },
  "content_scripts": [
      {
          "js": ["script.js"],
          "matches": ["<all_urls>"],
          "run_at": "document_start"
      }
  ]
}